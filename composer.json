{
    "name": "laravelarab/tarjama",
    "description": "It's a laravel model columns translation manager",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-package",
        "translate",
        "eloquent",
        "model",
        "i8n",
        "translation",
        "tarjama",
        "multilingual"
    ],
    "homepage": "https://github.com/LaravelArab/tarjama",
    "authors": [
        {
            "name": "Abdellah Chadidi",
            "email": "chadidi97@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.5.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.5.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelArab\\Tarjama\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelArab\\Tarjama\\TarjamaServiceProvider"
            ]
        }
    }
}
